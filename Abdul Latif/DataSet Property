   <script type="text/javascript">

    const myData = document.getElementById("info");

    myData.dataset.name = "Turaan";
    myData.dataset.age = 30;
    myData.dataset.livingIn = "Germany"

    // myData.removeAttribute('data-name');
    myData.removeAttribute('data-age');

